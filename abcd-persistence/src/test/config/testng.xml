<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="ABCDpersistence"
	preserve-order="true">
	<test name="formDao" enabled="true">
		<groups>
			<run>
				<include name="formDao" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.abcd.persistence.dao.FormTest" />
		</classes>
	</test>
	
	<test name="diagramDao" enabled="true">
		<groups>
			<run>
				<include name="diagramDao" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.abcd.persistence.dao.DiagramTest" />
		</classes>
	</test>
	
	<test name="formCustomVariablesDao" enabled="true">
		<groups>
			<run>
				<include name="formCustomVariablesDao" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.abcd.persistence.dao.FormCustomVariablesTest" />
		</classes>
	</test>
	
	<test name="globalVariablesDao" enabled="true">
		<groups>
			<run>
				<include name="globalVariablesDao" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.abcd.persistence.dao.GlobalVariablesTest" />
		</classes>
	</test>
	
	<test name="tableRulesDao" enabled="true">
		<groups>
			<run>
				<include name="tableRulesDao" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.abcd.persistence.dao.TableRuleTest" />
		</classes>
	</test>
	
	<test name="expressionEvaluator" enabled="true">
		<groups>
			<run>
				<include name="expressionEvaluator" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.jexeval.ExpressionTest" />
		</classes>
	</test>
	
	<test name="orphan" enabled="true">
		<groups>
			<run>
				<include name="orphan" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.abcd.persistence.dao.OrphanRemoveTest" />
		</classes>
	</test>
</suite>


